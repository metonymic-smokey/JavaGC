<?import at.jku.anttracks.gui.classification.component.configurationpane.ConfigurationPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<fx:root type="ConfigurationPane" xmlns="http://javafx.com/javafx/8.0.60"
         xmlns:fx="http://javafx.com/fxml/1">
    <HBox fx:id="operationPane" spacing="5.0">
        <children>
            <Button fx:id="applyButton" text="Apply" onAction="#apply"></Button>
            <Button fx:id="cancelButton" text="Cancel" onAction="#cancel"></Button>
        </children>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
    </HBox>
</fx:root>