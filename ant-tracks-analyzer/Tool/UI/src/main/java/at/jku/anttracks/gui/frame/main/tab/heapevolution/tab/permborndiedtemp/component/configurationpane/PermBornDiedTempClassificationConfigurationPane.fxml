<?xml version="1.0" encoding="UTF-8"?>

<?import at.jku.anttracks.gui.classification.component.configurationpane.ConfigurationPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<fx:root type="ConfigurationPane" xmlns="http://javafx.com/javafx/8.0.60"
         xmlns:fx="http://javafx.com/fxml/1">
    <HBox spacing="10">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <Button fx:id="applyButton" mnemonicParsing="false" text="Apply"/>
        <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel"/>
    </HBox>
</fx:root>
