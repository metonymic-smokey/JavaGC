<?import at.jku.anttracks.gui.classification.component.selectionpane.ClassificationSelectionPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.*?>
<fx:root id="titled-pane" type="at.jku.anttracks.gui.classification.component.selectionpane.ClassificationSelectionPane" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
    <VBox spacing="6" HBox.hgrow="ALWAYS">
        <TitledPane id="available-single-cardinality-titled-pane" animated="false" collapsible="false" text="1 -&gt; 1" HBox.hgrow="ALWAYS">
            <FlowPane fx:id="availableSingleCardinalitySubPane" alignment="CENTER_LEFT" hgap="6" vgap="6" HBox.hgrow="ALWAYS"/>
        </TitledPane>
        <TitledPane id="available-multi-cardinality-titled-pane" animated="false" collapsible="false" text="1 -&gt; *" HBox.hgrow="ALWAYS">
            <FlowPane fx:id="availableMultiCardinalitySubPane" alignment="CENTER_LEFT" hgap="6" vgap="6" HBox.hgrow="ALWAYS"/>
        </TitledPane>
        <TitledPane id="available-tree-cardinality-titled-pane" animated="false" collapsible="false" text="1 -&gt; [1:n]" HBox.hgrow="ALWAYS">
            <FlowPane fx:id="availableTreeCardinalitySubPane" alignment="CENTER_LEFT" hgap="6" vgap="6" HBox.hgrow="ALWAYS"/>
        </TitledPane>
        <TitledPane id="available-multi-tree-cardinality-titled-pane" animated="false" collapsible="false" text="1 -&gt; [1:n]*" HBox.hgrow="ALWAYS">
            <FlowPane fx:id="availableMultiTreeCardinalitySubPane" alignment="CENTER_LEFT" hgap="6" vgap="6" HBox.hgrow="ALWAYS"/>
        </TitledPane>
    </VBox>
</fx:root>
