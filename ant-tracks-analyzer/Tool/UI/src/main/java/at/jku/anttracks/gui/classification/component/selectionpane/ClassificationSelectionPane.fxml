<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root id="titled-pane" text="Classifier" type="TitledPane" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" animated="false">
    <content>
        <VBox spacing="5.0">
            <HBox fx:id="availableSuperPane" spacing="5" alignment="CENTER_LEFT">
                <Button fx:id="availableAddButton" mnemonicParsing="false" text="+"/>
                <BorderPane HBox.hgrow="ALWAYS">
                    <center>
                        <HBox fx:id="availablePane"/>
                    </center>
                </BorderPane>
            </HBox>

            <Separator fx:id="separator"/>

            <HBox alignment="CENTER_LEFT" spacing="5.0">
                <Label fx:id="selectedLabel" text="Selected: " alignment="CENTER_LEFT"/>
                <BorderPane HBox.hgrow="ALWAYS">
                    <center>
                        <FlowPane fx:id="selectedPane" hgap="6.0" vgap="6.0" alignment="CENTER_LEFT"/>
                    </center>
                </BorderPane>
            </HBox>
        </VBox>
    </content>
</fx:root>
